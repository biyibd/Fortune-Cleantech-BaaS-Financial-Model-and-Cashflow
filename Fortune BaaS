            <!-- Cost Structure Sheet -->
            <div id="costs" class="sheet">
                <h2 class="section-title">Comprehensive Cost Structure</h2>
                
                <div class="assumptions">
                    <h3>💸 Cost Components Overview</h3>
                    <div style="display: grid; grid-template-columns: 1fr 1fr; gap: 20px;">
                        <div>
                            <h4>Operating Costs (Annual)</h4>
                            <ul>
                                <li>Energy Costs: $71/EV/year</li>
                                <li>Battery Lease: $336.84/battery/year</li>
                                <li>Station OpEx: $5,000 per 10 stations</li>
                                <li>Overhead: 5% of revenue (min $42.5K)</li>
                            </ul>
                        </div>
                        <div>
                            <h4>Capital Expenditure</h4>
                            <ul>
                                <li>Station CapEx: $9,000/station</li>
                                <li>Battery Deposits: $85.50/battery</li>
                                <li>Depreciation: 5-year straight-line</li>
                            </ul>
                        </div>
                    </div>
                </div>
                
